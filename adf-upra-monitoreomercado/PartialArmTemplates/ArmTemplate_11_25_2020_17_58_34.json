				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "TRANSACCIONES_INMOBILIARIAS.CSV",
						"folderPath": "OTROS/SNR/TRANSACCIONES",
						"fileSystem": "raw"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "COD_DPTO",
						"type": "String"
					},
					{
						"name": "COD_DANE",
						"type": "String"
