					},
					{
						"name": "NRO_PROPIETARIOS",
						"type": "String"
					},
					{
						"name": "ESTADO_FOLIO",
						"type": "String"
					},
					{
						"name": "A�O RADICACION",
						"type": "String"
					},
					{
						"name": "LINDERO",
						"type": "String"
					},
					{
						"name": "SMMLV",
						"type": "String"
					},
					{
						"name": "RANGOS_AVALUOS",
						"type": "String"
					}
				]
			},
			"dependsOn": []
